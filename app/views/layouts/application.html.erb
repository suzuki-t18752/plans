<!DOCTYPE html>
<html>
  <head>
    <title>Plans</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "nav", :media => "all" %>
    <%= stylesheet_link_tag "toppage", :media => "all" %>
    <%= stylesheet_link_tag "user", :media => "all" %>
    <%= stylesheet_link_tag "plan", :media => "all" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/nav' %>
    
    <div class="main">
      <%= render 'layouts/flash_messages' %>
      
      <%= yield %>
    </div>
  </body>
</html>

